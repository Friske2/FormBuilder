{
  "name": "@friske2/form-builder",
  "version": "1.0.1",
  "type": "module",
  "description": "A Vue 3 dynamic form builder component with Element Plus",
  "author": "",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "form",
    "form-builder",
    "element-plus",
    "dynamic-form",
    "json-schema"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "main": "./dist/form-builder.umd.js",
  "module": "./dist/form-builder.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/form-builder.es.js",
      "require": "./dist/form-builder.umd.js"
    },
    "./style.css": "./dist/form-builder.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vue-tsc -b && vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib",
    "prepare": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "cleave.js": "^1.6.0",
    "element-plus": "^2.9.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/cleave.js": "^1.4.12",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "daisyui": "^5.0.43",
    "element-plus": "^2.9.11",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.8"
  }
}
